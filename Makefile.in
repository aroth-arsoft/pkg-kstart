# $Id$

CC		= @CC@
CFLAGS		= @CFLAGS@
CPPFLAGS	= -I. @CPPFLAGS@
LDFLAGS		= @LDFLAGS@
LIBS		= -lkrb -ldes @LIBS@

prefix		= /usr/local
bindir		= $(prefix)/bin

VPATH		= @srcdir@
srcdir		= @srcdir@

all: kstart

kstart: kstart.o 
	$(CC) $(LDFLAGS) -o kstart kstart.o $(LIBS)

install: kstart
	cp kstart $(bindir)/kstart

clean:
	rm -f *.o kstart

distclean: clean
	rm -f Makefile config.cache config.h config.log config.status

maintclean: distclean
	rm -f config.h.in configure
